class Student{
	char[] name = new char[] {'s', 'i', 'v', 'a'};
	//String name
	
	public boolean isHavingMiddleName(){
		int spaces = 0;
		for (char c : name ){
			if( c == ' ') spaces++;
		}
		if( spaces == 2 )return true;
		return false;
	}
}

class String{
	char ch[];
	
	public boolean isStartsWith(String string);
	public String replace(String string, char c);
	
}


